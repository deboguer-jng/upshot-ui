{
  "compilerOptions": {
    "jsx": "react",
    "module": "es2020",
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "./dist",
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "resolveJsonModule": true
  }
}
